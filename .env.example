# ESP32 SIP Door Station Configuration
# Copy this file to .env and customize the values for your setup

# Wi-Fi Configuration
WIFI_SSID=YourWiFiNetwork
WIFI_PASSWORD=YourWiFiPassword

# SIP Configuration
SIP_USER=doorstation
SIP_DOMAIN=192.168.1.100
SIP_PASSWORD=YourSIPPassword
SIP_CALLEE=sip:resident@192.168.1.100

# Web Server Configuration
WEB_PORT=8080

# Hardware Configuration
DOOR_PULSE_DURATION=2000